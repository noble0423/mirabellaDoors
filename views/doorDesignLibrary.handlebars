<!-- Non-CAD Design Image Right -->
<div class="my-0 my-sm-6">
    {{> images-components/images-component-right }}
</div>

<!-- Non-CAD Design Image Left -->
<div class="slant-background py-6">
    {{> images-components/images-component-left }}
</div>

<div class="my-0 my-sm-6">
    {{> images-components/images-component-right 
    partialContextTop = imagesComponentRight.smallImage.topRow.[2] 
    partialContextBottom = imagesComponentRight.smallImage.bottomRow.[2]
    partialContextSrc = imagesComponentRight.largeImage.[2]}}
</div>

<div class="slant-background py-6">
    {{> images-components/images-component-left 
    partialContextTop = imagesComponentLeft.smallImage.topRow.[3] 
    partialContextBottom = imagesComponentLeft.smallImage.bottomRow.[3]
    partialContextSrc = imagesComponentRight.largeImage.[3]}}
</div>

